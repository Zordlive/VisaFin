{
  "name": "cryptoinvest-backend",
  "version": "1.0.0",
  "description": "CryptoInvest backend - Express.js",
  "author": "CryptoInvest Team",
  "private": true,
  "license": "MIT",
  "main": "dist/start.js",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc src/server.ts src/start.ts --outDir dist --module commonjs --target es2020",
    "start": "node dist/start.js",
    "dev": "ts-node src/start.ts",
    "dev:watch": "nodemon --exec ts-node src/start.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix || true",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
